
{{#each this}}
    <li class="card">
        <div class="photo-card">
            <div class="photo-frame">
            <img src="{{webformatURL}}" data-source="{{largeImageURL}}" data-index="{{@index}}" alt="Picture from the gallery" 
            width="{{webformatWidth}}" height="{{webformatHeight}}">
                </div>
                <div class="stats">
                    <p class="stats-item">
                        <i class="material-icons">thumb_up</i>
                        {{likes}}
                    </p>
                    <p class="stats-item">
                        <i class="material-icons">visibility</i>
                       {{views}}
                    </p>
                    <p class="stats-item">
                        <i class="material-icons">comment</i>
                        {{comments}}
                    </p>
                    <p class="stats-item">
                        <i class="material-icons">cloud_download</i>
                        {{downloads}}
                    </p>
                </div>
        </div>
    </li>

{{/each}}